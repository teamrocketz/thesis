{
  "manifest_version": 2,

  "name": "Name!:) !",
  "description": "Securely saves your history to your own database",
  "version": "0.1",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "lock.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "lock.png"},
  "incognito": "split",
  "permissions": [
    "background",
    "browsingData",
    "cookies",
    "geolocation",
    "history",
    "identity",
    "sessions",
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation"
  ]
}